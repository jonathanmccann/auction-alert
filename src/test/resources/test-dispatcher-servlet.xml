<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<context:component-scan	base-package="com.app" />

	<bean id="viewResolver" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="prefix" value="/WEB-INF/jsp/" />
		<property name="suffix" value=".jsp" />
	</bean>

	<bean id="categoryDAO" class="com.app.dao.CategoryDAO" />
	<bean id="releaseDaoImpl" class="com.app.dao.impl.ReleaseDAOImpl" />
	<bean id="searchQueryDAOImpl" class="com.app.dao.impl.SearchQueryDAOImpl" />
	<bean id="searchQueryPreviousResultDAOImpl" class="com.app.dao.impl.SearchQueryPreviousResultDAOImpl" />
	<bean id="searchResultDAOImpl" class="com.app.dao.impl.SearchResultDAOImpl" />
</beans>